mkdir /opt/app-root/moodleda

chown -R apache. /opt/app-root/moodleda
chmod -R 777 /opt/app-root/moodleda

#!/bin/bash
mv moodle.tar.gz  mv /opt/app-root/src/

# restore build artifacts
if [ "$(ls /tmp/s2i/artifacts/ 2>/dev/null)" ]; then
    mv /tmp/s2i/artifacts/* $HOME/.
fi

#mover el moodle
mv /tmp/s2i/src $HOME/src

# build application artifacts
pushd ${HOME}
#make all

# install the artifacts
#make install
#popd
